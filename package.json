{
  "name": "webapp-v2",
  "version": "1.0.46",
  "description": "",
  "scripts": {
    "build-debug": "webpack --bail --type build  --env prod --progress",
    "build": "rm -rf dist && webpack --bail -p --type build  --env local --progress && npm run release-v1",
    "build-dev": "rm -rf dist && webpack --bail -p --type build  --env dev --progress && npm run release-v1",
    "build-demo": "rm -rf dist && webpack --bail -p --type build  --env demo --progress && npm run release-v1",
    "build-prod": "rm -rf dist && webpack --bail -p --type build  --env prod --progress && npm run release-v1",
    "serve": "webpack-dev-server --inline --type serve",
    "serve:dev": "webpack-dev-server --env dev --type serve --inline",
    "serve:prod": "webpack-dev-server --env prod --type serve --inline",
    "test": "pwd",
    "test:live": "karma start --type test --auto-watch --no-single-run",
    "prepublish": "npm run build",
    "release-v1": "node scripts/v1.js",
    "package": "node scripts/package.js",
    "release": "rm -rf dist && webpack --bail --type release  --env local --progress && npm run release-v1 && npm run package",
    "release-dev": "rm -rf dist && webpack --bail --type release  --env dev --progress && npm run release-v1 && npm run package",
    "release-demo": "rm -rf dist && webpack --bail --type release  --env demo --progress && npm run release-v1 && npm run package",
    "release-prod": "rm -rf dist && webpack --bail --type release  --env prod --progress && npm run release-v1 && npm run package",
    "gettext:compile": "angular-gettext-cli --compile --files './locale/*.po' --dest client/app/app.translations.js --format javascript --module gettext",
    "gettext:extract": "angular-gettext-cli --files \"./client/**/*.+(js|html)\" --dest ./locale/en.pot --marker-name i18n"
  },
  "license": "MIT",
  "dependencies": {
    "accounting": "^0.4.1",
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-bindonce": "^0.3.1",
    "angular-gettext": "^2.3.4",
    "angular-gettext-cli": "git+ssh://bitbucket.org/preoday/angular-gettext-cli.git",
    "angular-material": "1.0.9",
    "angular-material-calendar": "^0.2.14",
    "angular-material-data-table": "^0.10.10",
    "angular-messages": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-scroll": "^1.0.0",
