    "babel-polyfill": "^6.16.0",
    "chart.js": "2.4.0",
    "croppie": "^2.2.0",
    "jscore": "git+ssh://git@bitbucket.org/preoday/javascript-core.git",
    "leaflet": "1.0.3",
    "leaflet-draw": "0.4.9",
    "mapquest-api": "0.0.2",
    "md-color-picker": "^0.2.6",
    "mdPickers": "git+ssh://git@bitbucket.org/preoday/mdpickers.git",
