    "commander": "2.9.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.21.0",
    "eslint": "1.10.3",
    "eslint-loader": "1.7.1",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "0.8.5",
